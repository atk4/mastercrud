{
  "name": "atk4/mastercrud",
  "type": "library",
  "description": "Multi-level CRUD system component for ATK UI",
  "keywords": [
    "crud",
    "multi",
    "reference",
    "recursive",
    "atk4",
    "agile",
    "agile ui",
    "data",
    "framework"
  ],
  "homepage": "https://github.com/atk4/multicrud",
  "license": "MIT",
  "authors": [
    {
      "name": "Romans Malinovskis",
      "email": "romans@agiletoolkit.org",
      "homepage": "https://nearly.guru/"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "require": {
    "php": ">=7.3.0",
    "atk4/ui": "dev-develop"
  },
  "require-release": {
    "php": ">=7.3.0",
    "atk4/ui": "^2.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.16",
    "phpunit/phpcov": "*",
    "phpunit/phpunit": "*"
  },
  "autoload": {
    "psr-4": {
      "atk4\\mastercrud\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "atk4\\mastercrud\\tests\\": "tests/"
    }
  }
}
