{
    "name": "atk4/mastercrud",
    "type": "library",
    "description": "Multi-level CRUD system component for ATK UI",
    "keywords": ["crud", "multi", "reference", "recursive", "atk4", "agile", "agile ui", "data", "framework"],
    "homepage": "https://github.com/atk4/multicrud",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Romans Malinovskis",
            "email": "romans@agiletoolkit.org",
            "homepage": "https://nearly.guru/"
        }
    ],
    "require": {
        "atk4/ui": "^1.6.1",
        "atk4/data": "^1.3.7",
        "php": ">=7.0.0"
    },
    "autoload": {
        "psr-4": {"atk4\\mastercrud\\":"src/"}
    },
    "autoload-dev": {
        "psr-4": {
          "atk4\\mastercrud\\tests\\":"tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "<6"
    }
}
