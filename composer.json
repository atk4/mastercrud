{
    "name": "atk4/mastercrud",
    "description": "Multi-level CRUD system component for ATK UI",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "crud",
        "multi",
        "reference",
        "recursive",
        "atk4",
        "agile",
        "agile ui",
        "data",
        "framework"
    ],
    "version": "dev-develop",
    "authors": [
        {
            "name": "Romans Malinovskis",
            "homepage": "https://nearly.guru/"
        }
    ],
    "homepage": "https://github.com/atk4/multicrud",
    "require": {
        "php": ">=7.4 <8.4",
        "atk4/ui": "dev-develop"
    },
    "require-release": {
        "php": ">=7.4 <8.4",
        "atk4/ui": "~5.1.0"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.13",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^1.0",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-strict-rules": "^1.3"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Atk4\\MasterCrud\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Atk4\\MasterCrud\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "phpstan/extension-installer": true
        },
        "sort-packages": true
    }
}
